{
  "version": 2,
  "installCommand": "npm install --prefix ./frontend && npm install --prefix ./backend",
  "buildCommand": "npm run build --prefix ./frontend",
  "outputDirectory": "frontend/dist",
  "public": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
